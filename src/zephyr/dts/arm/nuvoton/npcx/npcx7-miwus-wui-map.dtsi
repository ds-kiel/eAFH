	/* Pseudo MIWU device to present no mapping relationship */
	miwu_none: miwu_pseudo {
		compatible = "nuvoton,npcx-miwu";
		index = <3>;
		#miwu-cells = <2>;
		label="MIWU_NONE";
		status = "disabled";
	};
};
