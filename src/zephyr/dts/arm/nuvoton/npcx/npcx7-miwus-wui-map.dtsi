		wui_vw_sus_stat:     wui2_1_4 { miwus = <&miwu2 0 4>; };
		wui_vw_plt_rst:      wui2_1_5 { miwus = <&miwu2 0 5>; };
		wui_vw_oob_rst_warn: wui2_1_6 { miwus = <&miwu2 0 6>; };

		/* MIWU group B */
