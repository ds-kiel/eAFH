#include <mem.h>
#include <nordic/nrf5340_cpunet.dtsi>

&flash1 {
	reg = <0x01000000 DT_SIZE_K(256)>;
};

&sram1 {
	reg = <0x21000000 DT_SIZE_K(64)>;
};

&mpu {
	arm,num-mpu-regions = <16>;
};

/ {
	soc {
		compatible = "nordic,nRF5340-CPUNET-QKAA", "nordic,nRF5340-CPUNET", "nordic,nRF53", "simple-bus";
	};
};
